#include<stdio.h>
int main()
{
	int n;
	printf("enter the array of size : ");
	scanf("%d",&n);
	int num[n],o=0,e=0;
	
	for(int i=0;i<n;i++)
	{
		printf("enter element %d values : ",i++);
		scanf("%d",&num[i]);
		if(num[i]%2!=0)
		{
			o+=num[i];
		}
		else
		{
			e+=num[i];
		}
	}
	
	printf("odd value element sum : %d\n even value element sum : %d ",o,e);
	return 0;
}